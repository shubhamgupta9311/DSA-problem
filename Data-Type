
class Solution {
    static int dataTypeSize(String str) {
     switch(str){
         case "Character":
             return 2;     //in java character take 2 byte ,in c/c++ it will be 1.
         case "Integer"  :
             return 4;
        case "Long":
            return 8;
        case "Float" :
            return 4;
        case "Double" :
            return 8;
        default :
        return -1;
         
         
         
         
     }
    
    }
}
